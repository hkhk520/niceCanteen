.rules-item-box{
  color: #333;
  font-size: 26rpx;
}
.rules-item-box .title{
  margin-bottom: 12rpx;
}
.rules-item .rule-text{
  display: inline-block;
  padding: 16rpx 0;
  width: 220rpx;
  text-align: center;
  background-color: #f8f8f8;
  margin: 0 16rpx 16rpx 0;
  border-radius: 12rpx;
}
.clearfix::before{
  content: "";
  display: block;
  clear: both;
  overflow: hidden;
}
.active{
  color: #01b6fd;
  background-color: #e5f6ff !important;
}